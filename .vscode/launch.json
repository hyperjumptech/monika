{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Symon",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\bin\\run_dev",
      "args": [
        "--symonUrl",
        "http://localhost:3000/",
        "--symonKey",
        "dc5b4caa-46f1-435f-9e23-7141b9b1a85a"
      ],
      "sourceMap": true
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Run Dev",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\bin\\run_dev",
      "args": ["-c", "./monika.yml"],
      "sourceMap": true
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/run",
      "args": []
    },
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch with args",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/bin/run",
      "args": ["-i mock"]
    }
  ]
}
