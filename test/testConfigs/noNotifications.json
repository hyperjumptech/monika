{
  "interval": 0,
  "probes": [
    {
      "id": "1",
      "name": "something",
      "description": "some description",
      "request": {
        "method": "POST",
        "url": "https://something/login",
        "headers": {
          "Authorization": ""
        },
        "body": {
          "username": "someusername",
          "password": "somepassword"
        }
      },
      "trueThreshold": 2,
      "falseThreshold": 2,
      "alerts": ["status-not-2xx", "response-time-greater-than-200-ms"]
    }
  ]
}
