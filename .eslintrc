{
    "extends": ["oclif", "oclif-typescript", "prettier"],
    "rules": {
        "no-use-before-define": ["warn", "nofunc"],
        "unicorn/prefer-node-protocol": "off"
    },
    "overrides": [
        {
            "files": ["docs/**/*.js"],
            "rules": {
                "unicorn/filename-case": "off"
            }
        }
    ],
    "env": {"node": true},
    "ignorePatterns": ["*.d.ts"]
}
