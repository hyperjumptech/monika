{
  "interval": 0,
  "notifications": [
    {
      "id": "unique-id-mailgun",
      "type": "mailgun",
      "data": {
        "recipients": ["recipient1@example.com"],
        "apiKey": "YOUR_API_KEY",
        "domain": "YOUR_DOMAIN"
      }
    },
    {
      "id": "unique-id-sendgrid",
      "type": "sendgrid",
      "data": {
        "recipients": ["recipient1@example.com"],
        "apiKey": "YOUR_API_KEY"
      }
    },
    {
      "id": "unique-id-smtp",
      "type": "smtp",
      "data": {
        "recipients": ["recipient1@example.com"],
        "hostname": "smtp.example.com",
        "port": 222,
        "username": "smtpusername",
        "password": "smtppassword"
      }
    },
    {
      "id": "unique-id-webhook",
      "type": "webhook",
      "data": {
        "url": "https://example.com/webhook"
      }
    },
    {
      "id": "unique-id-slack",
      "type": "slack",
      "data": {
        "url": "https://example.com/webhook"
      }
    }
  ],
  "probes": [
    {
      "id": "1",
      "name": "Example",
      "description": "Probe",
      "request": {
        "method": "GET",
        "url": "https://worldtimeapi.org/api/ip",
        "timeout": 7000,
        "headers": {
          "Authorization": ""
        },
        "body": {
          "username": "someusername",
          "password": "somepassword"
        }
      },
      "trueThreshold": 3,
      "falseThreshold": 3,
      "alerts": ["status-not-2xx", "response-time-greater-than-400-ms"]
    }
  ]
}
